{
  "name": "website",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:local": "astro dev --host --port 4321 --strictPort",
    "dev:cms": "concurrently -k -n astro,proxy \"astro dev --host --port 4321 --strictPort\" \"decap-server --port 8081 --allowed-hosts=localhost:4321\"",
    "doctor:cms": "node scripts/decap-doctor.mjs",
    "dev:git": "cross-env NODE_OPTIONS=--trace-warnings concurrently -k -s first -n astro \"astro dev --host --port 4321 --strictPort\"",
    "port:kill:4321": "powershell -NoProfile -Command \"$p=(Get-NetTCPConnection -LocalPort 4321 -ErrorAction SilentlyContinue).OwningProcess; if($p){Stop-Process -Id $p -Force}\"",
    "port:kill:8081": "powershell -NoProfile -Command \"$p=(Get-NetTCPConnection -LocalPort 8081 -ErrorAction SilentlyContinue).OwningProcess; if($p){Stop-Process -Id $p -Force}\"",
    "clean:cache": "powershell -NoProfile -Command \"Remove-Item -Recurse -Force .astro, node_modules\\.vite -ErrorAction SilentlyContinue\"",
    "del:cache": "npm run port:kill:4321 && npm run port:kill:8081 && npm run clean:cache",
    "build": "astro build",
    "preview": "astro preview --host --port 4321",
    "check": "tsc -p . --noEmit",
    "check:env": "node --env-file=.env.local ./scripts/check-env.mjs",
    "cal:webhook:create": "node ../../scripts/cal-webhook.mjs create",
    "cal:webhook:update": "node ../../scripts/cal-webhook.mjs update",
    "cal:webhook:list": "node ../../scripts/cal-webhook.mjs list",
    "cms:proxy": "decap-server --port 8081",
    "cms:sanitize": "node scripts/decap-sanitize-config.mjs",
    "cms:inspect": "node scripts/decap-dedupe-inspect.mjs",
    "cms:preflight": "node scripts/decap-config-preflight.mjs",
    "preflight:sections": "node scripts/preflight-sections.mjs",
    "preflight:icons": "node scripts/preflight-icons.mjs",
    "migrate:media": "node scripts/migrate-media.mjs",
    "create:placeholders": "node scripts/create-placeholders.mjs",
    "preflight:media": "node scripts/preflight-media.mjs",
    "preflight:all": "node scripts/preflight-all.mjs",
    "build:iconify": "node scripts/build-iconify-bundle.mjs",
    "optimize:media": "node scripts/optimize-media.mjs",
    "perf:report": "astro build && npx rollup-plugin-visualizer dist/_astro/*.js --filename dist/bundle-analysis.html --open",
    "cms:seed:footer": "tsx scripts/cms/seed-footer.ts",
    "fix:dates": "node ./scripts/normalize-post-dates.mjs",
    "fix:content": "node ./scripts/normalize-post-fields.mjs",
    "content:pull": "node ./scripts/content-pull.mjs",
    "content:migrate": "node ./scripts/migrate-posts.mjs",
    "content:normalize-media": "node ./scripts/normalize-media-paths.mjs",
    "content:seed:about": "tsx ./scripts/content-seed-about.ts",
    "content:seed:bookme": "tsx ./scripts/content-seed-bookme.ts",
    "migrate:devscard": "tsx ./scripts/migrate-devscard-to-decap.ts",
    "migrate:one-skill": "tsx scripts/migrate-one-skill.ts",
    "migrate:ru:clone-en": "tsx scripts/clone-en-about-to-ru.ts",
    "migrate:ru:clone-en:dry": "tsx scripts/clone-en-about-to-ru.ts --dry-run",
    "migrate:about": "tsx scripts/migrate-about.ts",
    "content:sanitize": "tsx scripts/content-sanitize-frontmatter.ts",
    "content:format:dry": "tsx scripts/content-formatter.ts --dry-run --lang=en,ru --format=md --debug",
    "content:format": "npm run content:sanitize && tsx scripts/content-formatter.ts --write --lang=en,ru --format=md --backup --pretty",
    "content:doctor": "npm run content:sanitize && npm run content:format:dry",
    "icons:collect": "node ./scripts/icons/collect-icons.mjs",
    "icons:bundle": "node ./scripts/icons/build-bundle.mjs",
    "icons:rebuild": "npm run icons:collect && npm run icons:bundle",
    "dev:cms:about": "concurrently -k -n astro,proxy \"npm run dev -- --host --port 4321\" \"npm run cms:proxy\"",
    "dev": "astro dev --host --port 4321",
    "cms:dev": "decap-server --port 4322",
    "dev:all": "pwsh -NoProfile -ExecutionPolicy Bypass -File .\\scripts\\dev-tools.ps1",
    "kill-ports": "pwsh -NoProfile -ExecutionPolicy Bypass -File .\\scripts\\dev-tools.ps1 -NoCms"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=18.20.4",
    "npm": ">=9"
  },
  "dependencies": {
    "@astrojs/react": "4.4.0",
    "@iconify/react": "6.0.2",
    "astro-decap-cms-oauth": "0.5.1",
    "date-fns": "4.1.0",
    "iconify-icon": "3.0.1",
    "marked": "16.3.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "type-fest": "5.0.1"
  },
  "devDependencies": {
    "@astrojs/node": "9.4.4",
    "@astrojs/tailwind": "6.0.2",
    "@auth/core": "0.40.0",
    "@iconify-json/fa6-brands": "1.2.6",
    "@iconify-json/fa6-solid": "1.2.4",
    "@iconify-json/logos": "1.2.9",
    "@iconify-json/mdi": "1.2.3",
    "@iconify-json/simple-icons": "1.2.54",
    "@iconify-json/twemoji": "1.2.4",
    "@tailwindcss/postcss": "4.1.13",
    "@tailwindcss/vite": "4.1.13",
    "@types/node": "24.6.1",
    "astro": "5.14.1",
    "auth-astro": "4.2.0",
    "autoprefixer": "10.4.21",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "decap-cms": "3.8.4",
    "decap-server": "3.3.1",
    "dotenv": "17.2.2",
    "fast-glob": "3.3.2",
    "gray-matter": "4.0.3",
    "js-yaml": "4.1.0",
    "postcss": "8.4.47",
    "rollup-plugin-visualizer": "6.0.4",
    "tailwindcss": "4.1.13",
    "toml": "3.0.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "yaml": "2.5.0",
    "yargs": "17.7.2",
    "zod": "3.23.8"
  }
}
